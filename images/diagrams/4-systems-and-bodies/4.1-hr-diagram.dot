digraph HRDiagram {
    // Graph settings
    bgcolor="#1a1a2e"
    fontname="Helvetica"
    fontcolor="white"
    label="Star Classification in Aurora C# (HR Diagram Concept)"
    labelloc=t
    fontsize=16
    rankdir=LR
    nodesep=0.3
    ranksep=0.6
    pad=0.5

    // Default node style
    node [
        fontname="Helvetica"
        fontsize=10
        fontcolor="white"
        style="filled"
        shape=box
    ]

    // Default edge style
    edge [
        style=invis
    ]

    // Main Sequence stars (most common)
    subgraph cluster_main {
        label="Main Sequence (V) - Primary Star Types"
        fontcolor="#95d5b2"
        fontsize=11
        color="#95d5b2"
        style=dashed

        ms_o [
            label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="2">
                <TR><TD><B>O-type</B></TD></TR>
                <TR><TD><FONT POINT-SIZE="8">30,000-50,000K</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="9" COLOR="#ffe66d">0.2%</FONT></TD></TR>
            </TABLE>>
            fillcolor="#6699ff"
            width=1.2
            color="#4ecdc4"
        ]
        ms_b [
            label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="2">
                <TR><TD><B>B-type</B></TD></TR>
                <TR><TD><FONT POINT-SIZE="8">11,000-28,000K</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="9" COLOR="#ffe66d">0.3%</FONT></TD></TR>
            </TABLE>>
            fillcolor="#99bbff"
            width=1.2
            color="#4ecdc4"
        ]
        ms_a [
            label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="2">
                <TR><TD><B>A-type</B></TD></TR>
                <TR><TD><FONT POINT-SIZE="8">7,750-10,000K</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="9" COLOR="#ffe66d">5.1%</FONT></TD></TR>
            </TABLE>>
            fillcolor="#ffffff"
            fontcolor="#1a1a2e"
            width=1.2
            color="#4ecdc4"
        ]
        ms_f [
            label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="2">
                <TR><TD><B>F-type</B></TD></TR>
                <TR><TD><FONT POINT-SIZE="8">6,150-7,500K</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="9" COLOR="#ffe66d">10.1%</FONT></TD></TR>
            </TABLE>>
            fillcolor="#ffffcc"
            fontcolor="#1a1a2e"
            width=1.2
            color="#4ecdc4"
        ]
        ms_g [
            label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="2">
                <TR><TD><FONT POINT-SIZE="12"><B>G-type (Sol)</B></FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="8">5,100-6,000K</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="10" COLOR="#95d5b2"><B>40.6%</B></FONT></TD></TR>
            </TABLE>>
            fillcolor="#ffff66"
            fontcolor="#1a1a2e"
            width=1.6
            color="#95d5b2"
            penwidth=3
        ]
        ms_k [
            label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="2">
                <TR><TD><B>K-type</B></TD></TR>
                <TR><TD><FONT POINT-SIZE="8">3,650-5,000K</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="9" COLOR="#ffe66d">20.4%</FONT></TD></TR>
            </TABLE>>
            fillcolor="#ffcc66"
            fontcolor="#1a1a2e"
            width=1.4
            color="#4ecdc4"
        ]
        ms_m [
            label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="2">
                <TR><TD><B>M-type</B></TD></TR>
                <TR><TD><FONT POINT-SIZE="8">2,300-3,920K</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="9" COLOR="#ffe66d">12.3%</FONT></TD></TR>
            </TABLE>>
            fillcolor="#ff6633"
            width=1.3
            color="#4ecdc4"
        ]
    }

    // Special star types
    subgraph cluster_special {
        label="Special Star Types"
        fontcolor="#888888"
        fontsize=10
        color="#4ecdc4"
        style=dashed

        wd [
            label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="2">
                <TR><TD><B>White Dwarfs (D)</B></TD></TR>
                <TR><TD><FONT POINT-SIZE="8">4,000-30,000K</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="9" COLOR="#ffe66d">5.0%</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="8" COLOR="#888888">Collapsed remnants</FONT></TD></TR>
            </TABLE>>
            fillcolor="#ccccff"
            fontcolor="#1a1a2e"
            width=1.6
            color="#4ecdc4"
        ]

        bd [
            label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="2">
                <TR><TD><B>Brown Dwarfs (L,T,Y)</B></TD></TR>
                <TR><TD><FONT POINT-SIZE="8">400-2,200K</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="9" COLOR="#ffe66d">6.0%</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="8" COLOR="#888888">Failed stars</FONT></TD></TR>
            </TABLE>>
            fillcolor="#993300"
            width=1.6
            color="#4ecdc4"
        ]

        bh [
            label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="2">
                <TR><TD><B>Black Holes</B></TD></TR>
                <TR><TD><FONT POINT-SIZE="8">1-120 solar masses</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="9" COLOR="#ffe66d">1.5%</FONT></TD></TR>
                <TR><TD><FONT POINT-SIZE="8" COLOR="#888888">Navigation hubs (if massive)</FONT></TD></TR>
            </TABLE>>
            fillcolor="#1a1a2e"
            width=1.6
            color="#ff6b6b"
        ]
    }

    // Flow edges for main sequence (invisible, just for layout)
    ms_o -> ms_b -> ms_a -> ms_f -> ms_g -> ms_k -> ms_m

    // Legend / notes
    legend [
        label=<<TABLE BORDER="0" CELLBORDER="0" CELLSPACING="4">
            <TR><TD COLSPAN="2"><FONT COLOR="#ffe66d"><B>Aurora Generation Notes</B></FONT></TD></TR>
            <TR><TD ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#95d5b2">G-type dominates (40.6%)</FONT></TD><TD ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#888888">Deliberate game design</FONT></TD></TR>
            <TR><TD ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#888888">Real universe:</FONT></TD><TD ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#888888">M-type dominates (75%+)</FONT></TD></TR>
            <TR><TD ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#888888">Giants/Supergiants:</FONT></TD><TD ALIGN="LEFT"><FONT POINT-SIZE="9" COLOR="#888888">Also generated (rare)</FONT></TD></TR>
        </TABLE>>
        shape=note
        fillcolor="#2d2d44"
        color="#ffe66d"
        width=3.2
    ]

    ms_m -> wd [style=invis]
    wd -> bd -> bh [style=invis]
    bh -> legend [style=invis]
}
